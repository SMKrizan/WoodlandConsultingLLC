{
  "name": "woodlandconsultingllc",
  "version": "1.0.0",
  "description": "lighting design consulting firm",
  "main": "server/server.js",
  "dependencies": {
    "if-env": "^1.0.4",
    "ra-data-simple-rest": "^3.10.4",
    "react-admin": "^3.12.3"
  },
  "devDependencies": {
    "concurrently": "^5.3.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "cd server && npm start",
    "start:dev": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "test-admin": "concurrently \"cd server-json && npm run server\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "heroku-postbuild": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SMKrizan/WoodlandConsultingLLC.git"
  },
  "keywords": [
    "consulting",
    "lighting",
    "design",
    "engineering"
  ],
  "author": "Krizan Pryadka Grosshans Sibley",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SMKrizan/WoodlandConsultingLLC/issues"
  },
  "homepage": "https://github.com/SMKrizan/WoodlandConsultingLLC#readme"
}
